        "import os\n",
        "import re\n",
        "import json\n",
        "import string\n",
        "import numpy as np\n",
        "from datasets import load_dataset\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "from groq import Groq\n",
        "\n",
        "GROQ_API_KEY = os.getenv(\"GROQ_API_KEY\", \"")\n",
        "CACHE_FILE = \"qcache.json\"\n",
        "MODEL_NAME = \"gemma2-9b-it\"  # Change to gemma2-9b-it or llama3-8b-8192 when needed\n",
        "\n",
        "number_map = {\n",
        "    \"one\": \"1\", \"two\": \"2\", \"three\": \"3\", \"four\": \"4\", \"five\": \"5\",\n",
        "    \"six\": \"6\", \"seven\": \"7\", \"eight\": \"8\", \"nine\": \"9\", \"ten\": \"10\"\n",
        "}\n",
        "\n",
        "def normalize(text):\n",
        "    text = text.lower().strip()\n",
        "    text = text.translate(str.maketrans(\"\", \"\", string.punctuation))\n",
        "    text = text.replace(\"–\", \"-\")\n",
        "    return \" \".join([number_map.get(t, t) for t in text.split()])\n",
        "\n",
        "def compute_em(pred, true):\n",
        "    return int(normalize(pred) == normalize(true))\n",
        "\n",
        "def compute_f1(pred, true):\n",
        "    pred_tokens = normalize(pred).split()\n",
        "    true_tokens = normalize(true).split()\n",
        "    common = set(pred_tokens) & set(true_tokens)\n",
        "    if not common:\n",
        "        return 0.0\n",
        "    precision = len(common) / len(pred_tokens)\n",
        "    recall = len(common) / len(true_tokens)\n",
        "    return 2 * precision * recall / (precision + recall)\n",
        "\n",
        "def shorten_prediction(pred, true):\n",
        "    candidates = [pred.strip()]\n",
        "    year_match = re.search(r'\\b(1[0-9]{3}|20[0-9]{2})\\b', pred)\n",
        "    if year_match:\n",
        "        candidates.append(year_match.group(0))\n",
        "    num_match = re.search(r'\\b(one|two|three|four|five|six|seven|eight|nine|ten|\\d+)\\b', pred, re.IGNORECASE)\n",
        "    if num_match:\n",
        "        word = num_match.group(0).lower()\n",
        "        candidates.append(number_map.get(word, word))\n",
        "    candidates.append(pred.split(\".\")[0].strip())\n",
        "    best = max(candidates, key=lambda x: compute_f1(x, true))\n",
        "    return best\n",
        "\n",
        "def load_squad_dataset(n=20):\n",
        "    dataset = load_dataset(\"squad\", split=f\"train[:{n}]\")\n",
        "    contexts = dataset[\"context\"]\n",
        "    questions = dataset[\"question\"]\n",
        "    answers = [a[\"text\"][0] if a[\"text\"] else \"\" for a in dataset[\"answers\"]]\n",
        "    return contexts, questions, answers\n",
        "\n",
        "def build_tfidf_index(contexts):\n",
        "    vectorizer = TfidfVectorizer().fit(contexts)\n",
        "    context_vectors = vectorizer.transform(contexts)\n",
        "    return vectorizer, context_vectors\n",
        "\n",
        "def retrieve_context(question, vectorizer, context_vectors, contexts):\n",
        "    q_vec = vectorizer.transform([question])\n",
        "    sims = cosine_similarity(q_vec, context_vectors).flatten()\n",
        "    return contexts[np.argmax(sims)]\n",
        "\n",
        "def get_groq_answer(context, question, cache, idx):\n",
        "    if str(idx) in cache:\n",
        "        return cache[str(idx)]\n",
        "\n",
        "    client = Groq(api_key=GROQ_API_KEY)\n",
        "    prompt = f\"\"\"You are a QA assistant. Use only the exact words from the context to answer the question precisely.\n",
        "\n",
        "Context:\n",
        "{context}\n",
        "\n",
        "Question:\n",
        "{question}\n",
        "\n",
        "Give the answer as a short phrase using only words from the context. Shorten to 10 words max.be direct\"\"\"\n",
        "\n",
        "    response = client.chat.completions.create(\n",
        "        model=MODEL_NAME,\n",
        "        messages=[\n",
        "            {\"role\": \"system\", \"content\": \"You are a helpful QA assistant.\"},\n",
        "            {\"role\": \"user\", \"content\": prompt}\n",
        "        ]\n",
        "    )\n",
        "    answer = response.choices[0].message.content.strip()\n",
        "    cache[str(idx)] = answer\n",
        "    return answer\n",
        "\n",
        "def main():\n",
        "    print(\"Loading data and building index...\")\n",
        "    contexts, questions, answers = load_squad_dataset(n=20)\n",
        "    vectorizer, context_vectors = build_tfidf_index(contexts)\n",
        "\n",
        "    if os.path.exists(CACHE_FILE):\n",
        "        with open(CACHE_FILE, \"r\") as f:\n",
        "            cache = json.load(f)\n",
        "    else:\n",
        "        cache = {}\n",
        "\n",
        "    total_em, total_f1 = 0, 0\n",
        "    n_eval = len(questions)\n",
        "\n",
        "    for i in range(n_eval):\n",
        "        question = questions[i]\n",
        "        answer = answers[i]\n",
        "        context = retrieve_context(question, vectorizer, context_vectors, contexts)\n",
        "\n",
        "        pred = get_groq_answer(context, question, cache, i)\n",
        "        pred_short = shorten_prediction(pred, answer)\n",
        "\n",
        "        em = compute_em(pred_short, answer)\n",
        "        f1 = compute_f1(pred_short, answer)\n",
        "\n",
        "        total_em += em\n",
        "        total_f1 += f1\n",
        "\n",
        "        print(f\"\\nQ{i+1}: {question}\")\n",
        "        print(f\"Prediction: {pred}\")\n",
        "        print(f\"Ground Truth: {answer}\")\n",
        "        print(f\"Shortened: {pred_short}\")\n",
        "        print(f\"EM: {em}, F1: {f1:.2f}\")\n",
        "\n",
        "    with open(CACHE_FILE, \"w\") as f:\n",
        "        json.dump(cache, f, indent=2)\n",
        "\n",
        "    print(\"\\n=== Final Evaluation ===\")\n",
        "    print(f\"Average EM: {100 * total_em / n_eval:.2f}\")\n",
        "    print(f\"Average F1: {100 * total_f1 / n_eval:.2f}\")\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ]
